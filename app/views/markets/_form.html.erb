<%= simple_form_for(@market) do |f| %>
  <%= f.input :name, label: "Nome" %>
  <%= f.input :inscription, label: "Matrícula" %>
  <%= f.input :weekday, label: "Dia da semana", collection: [ "Domingo", "Segunda", "Terça", "Quarta", "Quinta", "Sexta", "Sábado"] %>
  <%= f.input :address, label: "Endereço" %>
  <%= f.button :submit, class: "btn btn-primary"  %>
<% end %>
